<ion-view title="Polling Station Manager" hide-back-button="true">
  <ion-nav-buttons side="left">
    <button menu-toggle="left" class="button button-icon icon ion-navicon"></button>
  </ion-nav-buttons>
  <ion-nav-buttons side="right">
  </ion-nav-buttons>

  <ion-content>
      <div class="content list">
        <a href="#/app/openstation" class="category">
          <div class="icon-button ion-unlocked"></div>
          <div class = "menudiv"><span class = "submenutextspan">Open Station</span></div>
        </a>
        <a href="#/app/reportissue" class="category">
          <div class="icon-button ion-chatbox-working"></div>
           <div class = "menudiv"><span class = "submenutextspan">Report Issue</span></div>
        </a>
        <a href="#/app/progress" class="category">
          <div class="icon-button ion-clipboard"></div>
           <div class = "menudiv"><span class = "submenutextspan">Record Progress</span></div>
        </a>
        <a href="#/app/closestation" class="category">
          <div class="icon-button ion-locked"></div>
           <div class = "menudiv"><span class = "submenutextspan">Close Station</span></div>
        </a>
        <a href="#/app/tracking" class="category">
          <div class="icon-button ion-map"></div>
           <div class = "menudiv"><span class = "submenutextspan">Track Progress</span></div>
        </a>
        <a href="#/app/accounts" class="category">
          <div class="icon-button ion-calendar"></div>
           <div class = "menudiv"><span class = "submenutextspan">Accounts</span></div>
        </a>
       <!--  <a href="#/app/switchPS" class="category">
          <div class="icon-button ion-code-working"></div>
           <div class = "menudiv"><span class = "submenutextspan">Switch PS</span></div>
        </a> -->
        <a href="#/app/messages" class="category">
          <div class="icon-button ion-email"></div>
           <div class = "menudiv"><span class = "submenutextspan">Notifications</span></div>
        </a>
        <a href="#/app/photo" class="category">
          <div class="icon-button ion-camera"></div>
           <div class = "menudiv"><span class = "submenutextspan">Photo</span></div>
        </a>

      </div>


  </ion-content>

</ion-view>
