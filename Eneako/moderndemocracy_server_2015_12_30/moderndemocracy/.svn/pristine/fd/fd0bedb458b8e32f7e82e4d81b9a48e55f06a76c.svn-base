/*Grid Styles*/
.grid-col {
    box-sizing: border-box;
    float: left;
    padding-left: 3em;
    width: 100%;
}

.grid-wrap {
    clear: both;
    margin-left: -3em;
    overflow: hidden;
}
.grid-col-nopad {
    float: left;
    width: 100%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box; }

.col-full,
.col-set-full .grid-col {
    width: 100%; }

.col-one-half,
.col-set-one-half .grid-col {
    width: 50%; }

.col-one-third,
.col-set-one-third .grid-col {
    width: 33.333%; }

.col-one-quarter,
.col-set-one-quarter .grid-col {
    width: 25%; }

.col-two-thirds {
    width: 66.666%; }

.col-three-quarters {
    width: 75%; }

.col-one-sixth{
    width: 16.66%
}
.col-medium-col{
    width:43%;
} 
.col-small-col{
    width:8%;
}
.col-main-col{
    width:30%;
}

