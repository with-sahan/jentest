/* Modal */
.modal-body {
    overflow: hidden;
    font-size: 13px;
}
.modal-body .col-half {
    width: 50%;
    float: left;
}
.modal-body ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
}
.modal-body ul li {
    line-height: 2.1;
    padding-left: 26px;
    position: relative;
}
.modal-body ul li:before {
    font-size: 22px;
    font-family: FontAwesome;
    content: '\f1db';
    position: absolute;
    left: 0;
    top: 0;  
    line-height: 1.2;
    color: #cf616b;
}
.modal-body ul li.checked:before {
    color: #56c785;
    content: '\f058';
}
.modal-body form select {
    font-size: 14px;
    height: 36px;
    width: 100%;
}
.modal-body form textarea {
    width: 100%;
    height: 120px;
    resize: none;
}
.modal-header .close {
    opacity: .7;
    color: #fff;
    font-size: 24px;
}
.modal-header .close:hover {
    opacity: .9;
    color: #fff;
}

