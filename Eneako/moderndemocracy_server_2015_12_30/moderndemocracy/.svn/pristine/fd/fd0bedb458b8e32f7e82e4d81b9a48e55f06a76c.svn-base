nav {
    background: #69c4e8;
    background: -moz-linear-gradient(top, #69c4e8 0%, #21a1d4 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#69c4e8), color-stop(100%,#21a1d4));
    background: -webkit-linear-gradient(top, #69c4e8 0%,#21a1d4 100%);
    background: -o-linear-gradient(top, #69c4e8 0%,#21a1d4 100%);
    background: -ms-linear-gradient(top, #69c4e8 0%,#21a1d4 100%);
    background: linear-gradient(to bottom, #69c4e8 0%,#21a1d4 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#69c4e8', endColorstr='#21a1d4',GradientType=0 );

    -webkit-box-shadow: 0px 1px 2px 0px rgba(0,0,0,0.19);
    -moz-box-shadow: 0px 1px 2px 0px rgba(0,0,0,0.19);
    box-shadow: 0px 1px 2px 0px rgba(0,0,0,0.19);

    list-style-type: none;
    margin: 0 0 4;
    min-height: 10px;
    position:relative;
    z-index:2;
    width:97%;
}
nav ul {
    margin: 0;
}
nav {
    list-style: none outside none;
}
nav ul li {
    display: inline-block;
    height: 60px;
    line-height: 60px;
    width:20%;
    vertical-align:top;
    border-left:1px solid transparent;
    border-right:1px solid transparent;
}
nav ul li.active{
    background: linear-gradient(to bottom, #63b4d4, #2093c1) repeat scroll 0 0 rgba(0, 0, 0, 0);
    -webkit-box-shadow: inset 0px 0px 16px 0px rgba(0,0,0,0.19);
    -moz-box-shadow: inset 0px 0px 16px 0px rgba(0,0,0,0.19);
    box-shadow: inset 0px 0px 16px 0px rgba(0,0,0,0.19);
    border-left:1px solid #69c4e8;
    border-right:1px solid #69c4e8;
}
nav ul li a {
    color: #ffffff;
    font-size: 13px;
    font-weight: 700;
    letter-spacing: 1px;
    text-decoration: none;
    text-shadow: 0 1px 0 rgba(0, 0, 0, 0.25);
    text-transform: uppercase;
    transition: color 0.2s ease 0s;
    display:block;
}
nav.one-nav li {
    width: 100%;
}
a {
    text-decoration: none;
    color:#4F6B72;
    -webkit-transition: all 0.5s ease-in-out;
    -moz-transition: all 0.5s ease-in-out;
    -o-transition: all 0.5s ease-in-out;
    transition: all 0.5s ease-in-out;
}
nav ul li a:hover, nav ul li a:focus{color:#fff;text-decoration:none;}
